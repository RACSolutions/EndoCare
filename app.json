{
  "expo": {
    "name": "EndoCare",
    "slug": "endocare",
    "version": "1.2.2",
    "orientation": "portrait",
    "scheme": "endocare",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#b5407a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.racsolutions.endocare",
      "buildNumber": "5",
      "infoPlist": {
        "NSHealthShareUsageDescription": "EndoCare stores health data locally on your device to help you track endometriosis symptoms.",
        "NSHealthUpdateUsageDescription": "EndoCare stores health data locally on your device to help you track endometriosis symptoms.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#b5407a"
      },
      "package": "com.racsolutions.endocare",
      "versionCode": 4,
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "privacy": "https://ra-endo-care-app-pp.netlify.app/",
    "extra": {
      "eas": {
        "projectId": "c838bf60-a9f4-4025-b852-5478d54712d8"
      }
    }
  }
}